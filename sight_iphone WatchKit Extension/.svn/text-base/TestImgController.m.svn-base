#import "TestImgController.h"


@interface TestImgController()

@property (nonatomic, weak) IBOutlet WKInterfaceImage *cacheImg;
@property (nonatomic, weak) IBOutlet WKInterfaceImage *staticImg;
@property (nonatomic, weak) IBOutlet WKInterfaceImage *aniImg;
@property (nonatomic, weak) IBOutlet WKInterfaceButton *startBtn;
@property (nonatomic, weak) IBOutlet WKInterfaceButton *stopBtn;

@end


@implementation TestImgController

- (void)awakeWithContext:(id)context {
    [super awakeWithContext:context];
    
    // Configure interface objects here.
}

- (void)willActivate {
    // This method is called when watch view controller is about to be visible to user
    [super willActivate];
}

- (void)didDeactivate {
    // This method is called when watch view controller is no longer visible
    [super didDeactivate];
}

- (IBAction)playAnimation {
    // Uses images in WatchKit app bundle.
    [self.aniImg setImageNamed:@"Bus"];
    [self.aniImg startAnimating];
    
    // Animate with a specific range, duration, and repeat count.
    // [self.animatedImage startAnimatingWithImagesInRange:NSMakeRange(0, 4) duration:2.0 repeatCount:3];
}

- (IBAction)stopAnimation {
    [self.aniImg stopAnimating];
}

@end



