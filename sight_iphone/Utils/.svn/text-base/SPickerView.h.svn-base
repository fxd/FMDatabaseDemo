@property (nonatomic, copy) SPickerViewBlock pickerViewBlock;

@property (nonatomic, strong) NSArray *pickerData;
@property (nonatomic, strong) NSMutableDictionary *selectDict;

- (void)show;
- (void)dismiss;

@end
