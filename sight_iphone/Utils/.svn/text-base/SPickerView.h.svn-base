#import <UIKit/UIKit.h>

typedef NS_ENUM(NSUInteger, ToolBarButtonType) {
    EToolBarLeftButton = 0,
    EToolBarRightButton
};

typedef void(^SPickerViewBlock)(NSMutableDictionary *dict);

@interface SPickerView : UIView<UIPickerViewDelegate, UIPickerViewDataSource>

@property (nonatomic, strong) UIPickerView *picker;
@property (nonatomic, strong) UIView *toolBar;
